{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4|^8.0",
    "aws/aws-sdk-php": "^3.99.4",
    "blade-ui-kit/blade-heroicons": "^1.0.1",
    "blade-ui-kit/blade-ui-kit": "^0.2.0",
    "calebporzio/sushi": "^v2.1.1",
    "doctrine/dbal": "^3.0",
    "eluceo/ical": "2.0.0",
    "fideloper/proxy": "^4.4.1",
    "friendsofphp/php-cs-fixer": "^v2.18.4",
    "fruitcake/laravel-cors": "^v2.0.3",
    "guzzlehttp/guzzle": "^7.3.0",
    "laravel/framework": "v8.35.1",
    "laravel/sanctum": "^v2.9.3",
    "laravel/scout": "^v8.6.0",
    "laravel/tinker": "^v2.6.1",
    "league/flysystem-aws-s3-v3": "^1.0.29",
    "livewire/livewire": "v2.4.2",
    "meilisearch/meilisearch-laravel-scout": "v0.12.5",
    "spatie/laravel-activitylog": "^3.17.0",
    "spatie/laravel-mail-preview": "4.0.1",
    "spatie/laravel-medialibrary": "^9.0.0",
    "spatie/laravel-permission": "^4.0.1",
    "spatie/laravel-query-builder": "^3.3.4",
    "spatie/laravel-ray": "^1.17.1",
    "spatie/laravel-tail": "^4.3.2",
    "squizlabs/php_codesniffer": "^3.5.8",
    "statamic/cms": "^3.1",
    "stevebauman/purify": "v4.0.0"
  },
  "require-dev": {
    "facade/ignition": "^2.7.0",
    "fakerphp/faker": "^v1.14.1",
    "laravel/sail": "v1.4.10",
    "mockery/mockery": "^1.4.3",
    "nunomaduro/collision": "^v5.3.0",
    "phpunit/phpunit": "^9.5.4",
    "barryvdh/laravel-debugbar": "^v3.5.2",
    "barryvdh/laravel-ide-helper": "v2.9.1",
    "beyondcode/laravel-dump-server": "^1.7.0",
    "jasonmccreary/laravel-test-assertions": "^v1.1.0",
    "laracasts/cypress": "^1.3.0",
    "laravel-shift/blueprint": "v1.22.0",
    "psalm/plugin-laravel": "^v1.4.2",
    "vimeo/psalm": "^4.7.0"
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "pre-update-cmd": [
      "Statamic\\Console\\Composer\\Scripts::preUpdateCmd"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan statamic:install --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
