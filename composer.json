{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "algolia/algoliasearch-client-php": "^1.18",
        "barryvdh/laravel-debugbar": "^2.4",
        "bugsnag/bugsnag-laravel": "^2.0",
        "consoletvs/charts": "5.*",
        "google/apiclient": "^2.2",
        "guzzlehttp/guzzle": "^6.3",
        "jenssegers/date": "^3.2",
        "jgrossi/corcel": "dev-dev",
        "ksubileau/color-thief-php": "^1.3",
        "laracasts/flash": "^3.0",
        "laravel-notification-channels/telegram": "^0.0.2",
        "laravel-notification-channels/twitter": "^1.0",
        "laravel/framework": "5.5.*",
        "laravel/scout": "^3.0",
        "laravel/socialite": "^3.0",
        "laravelcollective/html": "^5.4.0",
        "league/commonmark": "^0.15.4",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "~2.1.0",
        "nexmo/client": "^1.0",
        "nothingworks/blade-svg": "^0.2.1",
        "pbmedia/laravel-ffmpeg": "^1.1",
        "spatie/laravel-analytics": "^3.0",
        "spatie/laravel-backup": "^4.19",
        "spatie/laravel-collection-macros": "^2.7",
        "spatie/laravel-demo-mode": "^1.1",
        "spatie/laravel-google-calendar": "^2.0",
        "spatie/laravel-googletagmanager": "^2.3",
        "spatie/laravel-mailable-test": "^1.1",
        "spatie/laravel-medialibrary": "^5.0.0",
        "spatie/laravel-menu": "^2.1",
        "spatie/laravel-missing-page-redirector": "^1.3",
        "spatie/laravel-newsletter": "^3.7",
        "spatie/laravel-permission": "^2.4",
        "spatie/laravel-responsecache": "^3.2",
        "spatie/laravel-tail": "^1.4",
        "spatie/laravel-tinker-tools": "^1.0",
        "spatie/regex": "^1.1",
        "themsaid/laravel-mail-preview": "^1.2",
        "tightenco/ziggy": "^0.4.1",
        "fideloper/proxy": "~3.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/corcel/corcel"
        }
    ],
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "^2.6",
        "friendsofphp/php-cs-fixer": "^2.4",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "spatie/phpunit-watcher": "^1.3",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "fix-cs": [
            "vendor/bin/php-cs-fixer fix ."
        ],
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    }
}